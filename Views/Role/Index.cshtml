@model IEnumerable<WebApplication1.Models.Role>
@{
    ViewData["Title"] = "Role Index";
}
<h1>@ViewData["Title"]</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Role ID</th>
            <th>Role Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
            <tr>
                <td>@role.roleId</td>
                <td>@role.Name</td>
            </tr>
        }
    </tbody>
</table>